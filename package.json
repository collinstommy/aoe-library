{
	"name": "aoe-library",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"local:cms": "netlify-cms-proxy-server",
		"parse-data": "node ./lib/parse.js",
		"migrate": "prisma migrate dev --name init",
		"reset": "NODE_ENV=development prisma migrate reset dev --name init",
		"seed": "NODE_ENV=development prisma db seed",
		"studio": "prisma studio"
	},
	"prisma": {
		"seed": "tsx prisma/seed.ts"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.15.5",
		"autoprefixer": "^10.4.12",
		"cssnano": "^5.0.8",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"json-to-pretty-yaml": "^1.2.2",
		"netlify-cms-proxy-server": "^1.3.20",
		"postcss": "^8.4.18",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.7.0",
		"prettier-plugin-tailwindcss": "^0.2.0",
		"prisma": "^4.7.1",
		"svelte": "^3.44.0",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.8",
		"vite": "^4.0.0"
	},
	"dependencies": {
		"@auth/core": "^0.1.4",
		"@auth/sveltekit": "^0.1.6",
		"@clerk/clerk-js": "^4.38.4",
		"@clerk/clerk-sdk-node": "^4.8.6",
		"@erickruano/clerk-svelte": "^0.0.6",
		"@next-auth/prisma-adapter": "^1.0.5",
		"@prisma/client": "^4.8.0",
		"@sveltestack/svelte-query": "^1.6.0",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/ui": "^0.7.2",
		"dayjs": "^1.10.4",
		"fuse.js": "^6.6.2",
		"next-auth": "^4.18.6",
		"tsx": "^3.12.1"
	},
	"type": "module"
}
